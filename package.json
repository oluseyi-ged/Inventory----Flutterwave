{
  "name": "inventory",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --port 8081 --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "install-all": "yarn install && yarn pod",
    "pod": "cd ios && pod install && cd ..",
    "clean": "yarn clean-ios && yarn clean-android && yarn clean-node && watchman watch-del-all && rm -rf /tmp/metro-*",
    "clean-android": "rm -rf android/.gradle android/.idea android/build android/app/build",
    "clean-ios": "cd ios && pod deintegrate && cd .. && pod cache clean --all && rm -rf ios/build ios/Podfile.lock",
    "clean-node": "rm -rf node_modules yarn.lock",
    "clean-npm": "npm cache clean --force",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "mac-build": "cd android && ./gradlew assembleDebug && cd ..",
    "mobile": "~/Library/Android/sdk/tools/emulator -avd Pixel_5_API_31"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/drawer": "^6.4.4",
    "@react-navigation/material-top-tabs": "^6.2.3",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/react-native-global-props": "^1.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-config": "0.71.3",
    "react": "18.0.0",
    "react-hook-form": "^7.43.5",
    "react-native": "0.69.7",
    "react-native-currency-input": "^1.1.0",
    "react-native-dotenv": "^3.3.0",
    "react-native-env": "^0.0.6",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-global-props": "^1.1.5",
    "react-native-heroicons": "^3.2.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-pager-view": "^5.4.25",
    "react-native-paper": "5.0.0-rc.5",
    "react-native-reanimated": "^3.0.0-rc.4",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-selectmultiple-button": "^0.1.105",
    "react-native-svg": "^13.0.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tab-view": "^3.1.1",
    "react-navigation": "^4.4.4",
    "react-navigation-drawer": "^2.7.2",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "redux-toolkit": "^1.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^11.5.4",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "~18.0.14",
    "@types/react-native": "^0.69.3",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-expo": "^48.0.2",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
